#include<iostream.h>
#include<conio.h>
#include<stdio.h>
class max
{
public:
inline int maximum(int a,int b)
{
return a>b?a:b;
}
inline int minimum(int a,int b)
{
return a<b?a:b;
}
};
void main()
{
int a,b;
max m;
clrscr();
cout<<"\n Enter two number:";
cin>>a>>b;
cout<<"\n Number 1st: " <<a<< endl;
cout<<"\n Number 2nd: "<<b<< endl;
cout<<"\n Maximum number is: "<< m.maximum(a,b)<< endl;
cout<<"\n Minimum is: "<< m.minimum(a,b)<< endl;
getch();
}
